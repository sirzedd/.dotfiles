function tg-dev() {
  exportTerraformEnvironments
	readonly dev_full_arn="arn:aws:iam::<AWS_ENV_ID>:role/<ROLE_NAME>"
	terragrunt $@ --terragrunt-iam-role $dev_full_arn
}